<h1>Listing poi_data</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @poi_datas.each do |poi_data| %>
      <tr>
        <td><%= link_to 'Show', poi_data %></td>
        <td><%= link_to 'Edit', edit_poi_data_path(poi_data) %></td>
        <td><%= link_to 'Destroy', poi_data, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Poi datum', new_poi_data_path %>
