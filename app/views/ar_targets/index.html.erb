<h1>Listing ar_targets</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Image url</th>
      <th>Data url</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ar_targets.each do |ar_target| %>
      <tr>
        <td><%= ar_target.name %></td>
        <td><%= ar_target.image_url %></td>
        <td><%= ar_target.data_url %></td>
        <td><%= ar_target.description %></td>
        <td><%= link_to 'Show', ar_target %></td>
        <td><%= link_to 'Edit', edit_ar_target_path(ar_target) %></td>
        <td><%= link_to 'Destroy', ar_target, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ar target', new_ar_target_path %>
